<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="380.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.TeacherEditAddQuestion">
   <children>
      <Label fx:id="questionID" text="Edit Question:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox>
         <children>
            <Label text="Question:">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <TextArea fx:id="questionString" maxHeight="60.0" maxWidth="367.0" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox layoutX="50.0" layoutY="77.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <RadioButton fx:id="answer1" mnemonicParsing="false" text="RadioButton" wrapText="true">
               <toggleGroup>
                  <ToggleGroup fx:id="answers" />
               </toggleGroup>
            </RadioButton>
         </children>
      </HBox>
      <HBox layoutX="50.0" layoutY="177.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <RadioButton fx:id="answer2" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" wrapText="true" />
         </children>
      </HBox>
      <HBox layoutX="50.0" layoutY="177.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <RadioButton fx:id="answer3" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" wrapText="true" />
         </children>
      </HBox>
      <HBox layoutX="50.0" layoutY="177.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <RadioButton fx:id="answer4" mnemonicParsing="false" text="RadioButton" toggleGroup="$answers" wrapText="true" />
         </children>
      </HBox>
      <HBox layoutX="40.0" layoutY="278.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Field Selection" />
                  <ComboBox fx:id="fields" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Course Selection" />
                  <ScrollPane minViewportHeight="80.0" minViewportWidth="250.0" prefHeight="200.0" prefWidth="200.0">
                    <content>
                      <AnchorPane minHeight="80.0" minWidth="250.0">
                           <children>
                              <VBox fx:id="courseVbox" />
                           </children>
                        </AnchorPane>
                    </content>
                  </ScrollPane>
               </children>
               <HBox.margin>
                  <Insets left="70.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="backB" mnemonicParsing="false" onAction="#backToMenu" text="Back" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
