<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="380.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.TeacherTimeChangeRequest">
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
   <children>
      <Label prefHeight="38.0" prefWidth="246.0" text="Teacher Time Change Request">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </Label>
      <VBox alignment="CENTER" prefHeight="126.0" prefWidth="493.0">
         <children>
            <HBox fillHeight="false">
               <children>
                  <Label minWidth="-Infinity" text="Select new time :">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="SelectNewTime" prefHeight="25.0" prefWidth="44.0" promptText="(min)" />
               </children>
               <opaqueInsets>
                  <Insets top="10.0" />
               </opaqueInsets>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <Label fx:id="Errortime" text="*Time Field is Empty" textFill="RED" visible="false">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="150.0" prefWidth="493.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Request explenation:">
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="RequestExplenation" alignment="TOP_LEFT" prefHeight="80.0" prefWidth="372.0" />
               </children>
            </HBox>
            <Label fx:id="Errorrequest" text="*Exsplanation Text Field is empty" textFill="RED" visible="false" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER" prefHeight="23.0" prefWidth="493.0">
         <children>
            <Button mnemonicParsing="false" onAction="#CancelButtonPressed" prefHeight="25.0" prefWidth="50.0" text="Back">
               <HBox.margin>
                  <Insets right="200.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#SubmitRequest" text="Submit request" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <padding>
            <Insets top="20.0" />
         </padding>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets bottom="10.0" />
   </opaqueInsets>
</VBox>
